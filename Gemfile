source 'https://rubygems.org'
gem 'rails', '~> 5.0.0'
group :development, :test do
gem 'rspec-rails'
end
# Gems used only for assets and not required
# in production environments by default.
group :assets do
gem 'sass-rails'
gem 'coffee-rails'
gem 'uglifier'
end
gem 'jquery-rails'
group :test do
gem 'capybara'
end
group :production do
gem 'pg'
end
